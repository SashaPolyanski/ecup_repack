{
  "name": "ecup_repack",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "generate-types": "npx openapi-typescript https://ecup.pro/api/swagger/schema.yml -o src/api/schema/schema.ts",
    "my-awesome-script-name": "openapi-generator-cli generate -i https://ecup.pro/api/swagger/schema.yml -g typescript-redux-query -o generated-sources/openapi --additional-properties=ngVersion=6.1.7,npmName=restClient,supportsES6=true,npmVersion=6.9.0,withInterfaces=true"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "js-yaml": "^4.1.0",
    "openapi-typescript": "^6.7.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
}
